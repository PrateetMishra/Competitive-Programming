#include<bits/stdc++.h>
using namespace std;

int main() {
	int test_cases;
	cin >> test_cases;
	while (test_cases--) {
		int no_of_cars, min, counter=1;
		cin >> no_of_cars;
		int max_speed[no_of_cars];			
	
		for (int i = 0; i < no_of_cars; i++) {
			cin >> max_speed[i];
		}

		min = max_speed[0];
		for (int i = 1; i < no_of_cars; ++i)
		{
			if (max_speed[i] <= min) {
				counter++;
				min = max_speed[i];
			}
		}
		cout << counter << endl;
	}
}
